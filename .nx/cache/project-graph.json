          "dev": {
            "executor": "nx:run-script",
            "options": {
              "script": "dev"
            },
            "configurations": {}
          },
          "build": {
            "executor": "nx:run-commands",
            "dependsOn": [
              "^build"
            ],
            "inputs": [
              "production",
              "^production"
            ],
            "outputs": [
              "{workspaceRoot}/apps/backend/dist"
            ],
            "options": {
              "command": "bun build src/main.ts --target bun --outdir dist --external @nestjs/* --external reflect-metadata --external rxjs",
              "cwd": "apps/backend"
            },
            "configurations": {}
          },
          "start": {
            "executor": "nx:run-commands",
            "options": {
              "command": "bun dist/main.js",
              "cwd": "apps/backend"
            },
            "configurations": {}
          },
          "lint": {
            "executor": "nx:run-commands",
            "options": {
              "command": "bunx eslint . --ext .ts",
              "cwd": "apps/backend"
            },
            "configurations": {}
          },
          "db:generate": {
            "executor": "nx:run-script",
            "options": {
              "script": "db:generate"
            },
            "configurations": {}
          },
          "db:push": {
            "executor": "nx:run-script",
            "options": {
              "script": "db:push"
            },
            "configurations": {}
          },
          "db:studio": {
            "executor": "nx:run-script",
            "options": {
              "script": "db:studio"
            },
            "configurations": {}
          },
          "serve": {
            "executor": "nx:run-commands",
            "dependsOn": [
              "^build"
            ],
            "options": {
              "command": "bun --hot src/main.ts",
              "cwd": "apps/backend"
            },
            "configurations": {}
          }
        },
        "sourceRoot": "apps/backend/src",
        "implicitDependencies": [],
        "tags": []
