{
  "nodes": {
    "frontend": {
      "name": "frontend",
      "type": "lib",
      "data": {
        "root": "apps/frontend",
        "name": "frontend",
        "targets": {
          "dev": {
            "executor": "nx:run-script",
            "options": {
              "script": "dev"
            },
            "configurations": {}
          },
          "build": {
            "executor": "nx:run-commands",
            "dependsOn": [
              "^build"
            ],
            "inputs": [
              "production",
              "^production"
            ],
            "outputs": [
              "{workspaceRoot}/apps/frontend/dist"
            ],
            "options": {
              "command": "bunx vite build",
              "cwd": "apps/frontend"
            },
            "configurations": {}
          },
          "start": {
            "executor": "nx:run-commands",
            "options": {
              "command": "bun server.ts",
              "cwd": "apps/frontend"
            },
            "configurations": {}
          },
          "preview": {
            "executor": "nx:run-commands",
            "options": {
              "command": "bunx vite preview",
              "cwd": "apps/frontend"
            },
            "configurations": {}
          },
          "lint": {
            "executor": "nx:run-commands",
            "options": {
              "command": "bunx eslint . --ext .ts,.tsx",
              "cwd": "apps/frontend"
            },
            "configurations": {}
          },
          "serve": {
            "executor": "nx:run-commands",
            "dependsOn": [
              "^build"
            ],
            "options": {
              "command": "bunx vite",
              "cwd": "apps/frontend"
            },
            "configurations": {}
          }
        },
        "sourceRoot": "apps/frontend/src",
        "implicitDependencies": [],
        "tags": []
      }
    },
